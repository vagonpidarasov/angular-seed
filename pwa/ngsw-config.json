{
    "index": "/index.html",
    "dataGroups": [
        {
            "name": "images",
            "urls": [
                "*images.ctfassets.net*"
            ],
            "cacheConfig": {
                "strategy": "freshness"
            }
        }
    ],
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/index.html",
                "/vendor.*.js",
                "/main.*.js",
                "/vendor.*.css"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/*.woff",
                "/*.woff2"
            ]
        }
    }]
}
