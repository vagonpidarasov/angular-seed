{
    "index": "/index.html",
    "dataGroups": [
        {
            "name": "contentful",
            "urls": [
                "//cdn.contentful.com/spaces/**"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "3d",
                "timeout": "1m",
                "strategy": "freshness"
            }
        }
    ],
    "assetGroups": [{
        "name": "ctfassets",
        "resources": {
            "urls": [
                "//images.ctfassets.net/**"
            ]
        }
    }, {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/index.html",
                "/vendor.*.js",
                "/main.*.js",
                "/vendor.*.css"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/*.woff",
                "/*.woff2",
                "/*.ttf"
            ]
        }
    }]
}
